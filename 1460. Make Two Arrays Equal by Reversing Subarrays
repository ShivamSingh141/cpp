                   class Solution {
public:    
    bool canBeEqual(vector<int>& ta rget, vector<int>& arr) {   
         sort(arr.begin(),arr.end( ));      
        sort(target.begin(),target. end());        
        if(arr.size()!=target.size()){               
            return false;         
        }           
        for(int i=0;i<arr.size();i++){      
       

   
            if(arr[i]!=target[i]){
   
                return false;
            }
        }  
        return true;
        
    }
};
