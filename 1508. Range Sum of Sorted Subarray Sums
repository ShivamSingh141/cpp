     class Solution {
public:
int M=1e9+7;
    int rangeSum(vector<int>& nums, int n, int left, int right) {
        vector<int>temp;
        
        for(int i=0;i<nums.size();i++){ 
             int sum=0;   
            for(int j=i;j<nums.size();j++){  
                sum+=nums[j]; 
                temp.push_back(sum);  

            }   
        }
        int sum1=0;
        sort(temp.begin(),temp.end());
        for(int i=left-1;i<=right-1;i++){ 
            sum1=(sum1+temp[i])%M;
        }
        return sum1;
        
    }
};
